<%= form_for @photo, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :published %>
    <%= f.check_box :published %>
  </p>
  <p>
  	<%= f.label :image %>
  	<%= f.file_field :image %>
  </p>
  <p><%= f.submit %></p>
<% end %>
